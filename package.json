{
  "name": "obsidian-budget-planner-plugin",
  "version": "1.0.0",
  "description": "A custom code block plugin for Obsidian that provides a user-friendly table for budget planning.",
  "keywords": [
    "obsidian",
    "plugin",
    "budget",
    "planner",
    "table",
    "code block"
  ],
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "NODE_ENV=production vite build",
    "version": "node version-bump.js && git add manifest.json versions.json",
    "lint": "eslint --config eslint.config.js .",
    "lint:fix": "eslint . --ext .js,.ts,.svelte --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run"
  },
  "author": "Ivan Kalinichenko <joker.bsg@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "dependencies": {
    "nanoid": "^5.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.7",
    "@types/node": "^20.17.24",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@typescript-eslint/type-utils": "^8.26.0",
    "builtin-modules": "^5.0.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-svelte": "^3.0.3",
    "globals": "^16.0.0",
    "obsidian": "latest",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.25.7",
    "svelte-check": "^4.1.5",
    "svelte-eslint-parser": "^1.1.2",
    "svelte-preprocess": "^6.0.3",
    "tslib": "2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.2.5",
    "vitest": "^3.1.1"
  }
}
